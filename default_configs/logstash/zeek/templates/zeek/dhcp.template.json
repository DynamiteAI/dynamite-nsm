          "mapping": {
            "type": "float"
          }
        }
      },
      {
        "zeek.server_addr": {
          "path_match": "zeek.server_addr",
          "mapping": {
            "type": "ip"
          }
        }
      },
      {
        "zeek.msg_orig": {
          "path_match": "zeek.msg_orig",
          "mapping": {
            "type": "keyword"
          }
        }
