          "path_match": "zeek.uids",
          "mapping": {
            "type": "keyword"
          }
        }
      },
      {
        "zeek.client_addr": {
          "path_match": "zeek.client_addr",
          "mapping": {
            "type": "ip"
          }
        }
      },
      {
        "zeek.assigned_addr": {
          "path_match": "zeek.assigned_addr",
          "mapping": {
            "type": "ip"
          }
        }
      },
      {
        "zeek.ts": {
          "path_match": "zeek.ts",
          "mapping": {
            "type": "date"
          }
        }
      },
      {
        "zeek.msg_types": {
          "path_match": "zeek.msg_types",
          "mapping": {
            "type": "keyword"
          }
        }
