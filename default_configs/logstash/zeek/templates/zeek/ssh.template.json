          }
        }
      },
      {
        "zeek.id.resp_h": {
          "path_match": "zeek.id.resp_h",
          "mapping": {
            "type": "ip"
          }
        }
      },
      {
        "zeek.compression_alg": {
          "path_match": "zeek.compression_alg",
          "mapping": {
            "type": "keyword"
          }
        }
      },
      {
        "zeek.host_key": {
          "path_match": "zeek.host_key",
          "mapping": {
            "type": "keyword"
          }
        }
      },
      {
        "zeek.server": {
          "path_match": "zeek.server",
          "mapping": {
            "type": "keyword"
          }
        }
      },
      {
        "zeek.id.orig_h": {
          "path_match": "zeek.id.orig_h",
          "mapping": {
            "type": "ip"
          }
        }
      },
      {
        "zeek.id.resp_p": {
          "path_match": "zeek.id.resp_p",
          "mapping": {
            "type": "float"
          }
        }
      },
      {
        "zeek.client": {
          "path_match": "zeek.client",
          "mapping": {
            "type": "keyword"
          }
        }
      },
