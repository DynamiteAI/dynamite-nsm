        "zeek.cipher_alg": {
          "path_match": "zeek.cipher_alg",
          "mapping": {
            "type": "keyword"
          }
        }
      },
      {
        "zeek.direction": {
          "path_match": "zeek.direction",
          "mapping": {
            "type": "keyword"
          }
        }
      },
      {
        "zeek.uid": {
          "path_match": "zeek.uid",
          "mapping": {
            "type": "keyword"
          }
        }
      },
      {
