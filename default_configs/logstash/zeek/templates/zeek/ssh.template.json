      {
        "zeek.version": {
          "path_match": "zeek.version",
          "mapping": {
            "type": "float"
          }
        }
      },
      {
        "zeek.mac_alg": {
          "path_match": "zeek.mac_alg",
          "mapping": {
            "type": "keyword"
          }
        }
      },
      {
        "zeek.kex_alg": {
          "path_match": "zeek.kex_alg",
          "mapping": {
            "type": "keyword"
          }
        }
      },
      {
        "zeek.auth_attempts": {
          "path_match": "zeek.auth_attempts",
          "mapping": {
            "type": "float"
          }
        }
      }
    ]
